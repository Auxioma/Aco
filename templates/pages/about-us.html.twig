{% extends 'base.html.twig' %}

{% block title %}Entreprise de nettoyage, ACOPropreté{% endblock %}
{% block description %}Appuyez-vous sur une entreprise de nettoyage de confiance en propreté industrielle pour l'entretien de vos locaux professionnels.{% endblock %}

{% block body %}
    <!-- History -->
    <section class="ab-history-section pt-100 pb-100">
        <span class="ab-history-left-img"><img src="{{ asset('images/histoire/aco.png') }}" alt=""></span>
        <div class="container">
            <div class="ab-history-top">
                <div class="row align-items-center">
                    <div class="col-lg-6">
                        <div class="bixol-title-area">
                            <span class="bixol-subtitle">Notre histoire</span>
                            <h3>D'avril 2013 à <span>nos jours</span></h3>
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <div class="bf-desc"></div>
                    </div>
                </div>
            </div>
            <div class="ab-history-tab">
                <div class="tab-control">
                    <ul class="nav nav-pills">
                        <li><a href="#seventeen" data-bs-toggle="pill" class="active">2013</a></li>
                        <li><a href="#eightteen" data-bs-toggle="pill">2018</a></li>
                        <li><a href="#nineteen" data-bs-toggle="pill">2019</a></li>
                        <li><a href="#tweenty" data-bs-toggle="pill">{{ "now"|date("Y") }}</a></li>
                    </ul>
                    <div class="tab-devider"><hr></div>
                </div>
                <div class="tab-content">
                    <div class="tab-pane fade show active" id="seventeen">
                        <div class="history-content">
                            <h4>La création de notre entreprise avec  <br>avec ses experts nettoyage</h4>
                            <p>We know that if you love our service you’re going to recommend us to your family and friends, so your satisfaction is our number one priority. If you’re unhappy with our service in any way.</p>
                        </div>
                    </div>
                    <div class="tab-pane fade" id="eightteen">
                        <div class="history-content">
                            <h4>Our Company starts first step <br>with our expert team!</h4>
                            <p>We know that if you love our service you’re going to recommend us to your family and friends, so your satisfaction is our number one priority. If you’re unhappy with our service in any way.</p>
                        </div>
                    </div>
                    <div class="tab-pane fade" id="nineteen">
                        <div class="history-content">
                            <h4>Our Company starts first step <br>with our expert team!</h4>
                            <p>We know that if you love our service you’re going to recommend us to your family and friends, so your satisfaction is our number one priority. If you’re unhappy with our service in any way.</p>
                        </div>
                    </div>
                    <div class="tab-pane fade" id="tweenty">
                        <div class="history-content">
                            <h4>Our Company starts first step <br>with our expert team!</h4>
                            <p>We know that if you love our service you’re going to recommend us to your family and friends, so your satisfaction is our number one priority. If you’re unhappy with our service in any way.</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- History Area End -->

    <!-- Services Section -->
    <section class="abv2-service-section pt-100" data-background="assets/images/about/service-bg.jpg">
        <div class="container">
            <div class="title-top">
                <div class="row align-items-center">
                    <div class="col-lg-12">
                        <div class="bixol-title-area">
                            <span class="bixol-subtitle">Experts en propreté</span>
                            <h3>Tous types d'espaces professionnels <span>Service fiable et efficace.</span></h3>
                        </div>
                    </div>
                </div>
            </div>
            <div class="abv2-sr-slider">
                {% for metier in metiers  %}
                <div class="abv2-sr-item">
                    <span class="item-count">0{{ loop.index }}</span>
                    <div class="icon-wrapper">
                        <span><i class="flaticonv2-hospital"></i></span>
                    </div>
                    <h5>{{ metier.titre |u.truncate(25, '...') }}</h5>
                    <p>{{ metier.description}}</p>
                    <div class="abv2-hv-item" data-background="{{ asset('images/metier/services/' ~ metier.slug ~ '.png') }}">
                        <a href="{{  path('app_metier', {'Slug': metier.Slug}) }}"><h5>{{ metier.titre |u.truncate(25, '...') }}</h5></a>
                        <p>{{ metier.description}}</p>
                        <a href="{{  path('app_metier', {'Slug': metier.Slug}) }}">{{ metier.titre }} <i class="fas fa-arrow-right"></i></a>
                        <span class="hv-item-count">0{{ loop.index }}</span>
                    </div>
                </div>
                {% endfor %}
            </div>
        </div>
    </section>
    <!-- Service Slider End -->

    <!-- Contact Section -->
    <section class="abv2-contact pt-250 pt-sm-100 pb-100">
        <div class="container">
            <div class="row">
                <div class="col-lg-6">
                    <div class="abv2-contact-left">
                        <div class="bixol-title-area">
                            <span class="bixol-subtitle">Contactez-nous</span>
                            <h3>N'hésitez pas à nous contacter <span>pour toute question.</span></h3>
                            <p>Nous vous remercions de l'intérêt que vous portez à l'entreprise de nettoyage ACOPROPRETE. Nous prenons en compte la communication avec le client.</p>
                        </div>
                        <div class="row">
                            <div class="col-md-6 col-sm-6">
                                <div class="abv2-contact-item">
                                    <div class="icon-wrapper">
                                        <span><i class="flaticon flaticon-phone"></i></span>
                                    </div>
                                    <div class="contact-content">
                                        <h5>Téléphone: </h5>
                                        <span>Bureau: +33 6 95 74 93 94</span>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6 col-sm-6">
                                <div class="abv2-contact-item">
                                    <div class="icon-wrapper">
                                        <span><i class="flaticon flaticon-mail"></i></span>
                                    </div>
                                    <div class="contact-content">
                                        <h5>Mail: </h5>
                                        <span>aco@acoproprete.com</span>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6 col-sm-6">
                                <div class="abv2-contact-item">
                                    <div class="icon-wrapper">
                                        <span><i class="flaticon flaticon-pin"></i></span>
                                    </div>
                                    <div class="contact-content">
                                        <h5>Adresse: </h5>
                                        <span>1, Rue des Peupliers</span>
                                        <span>74500, <strong>Lugrin</strong></span>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6 col-sm-6">
                                <div class="abv2-contact-item">
                                    <div class="icon-wrapper">
                                        <span><i class="flaticon flaticon-alarm-clock"></i></span>
                                    </div>
                                    <div class="contact-content">
                                        <h5>Nos horaires: </h5>
                                        <span>9:00 - 18:00</span>
                                        <span class="text-grey">(Lundi au vendredi)</span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-6">
                    <div class="abv2-contact-right">
                        {{ form_start(form) }}
                            <div class="name-area">
                                <div class="fname">
                                    {{ form_widget(form.nom) }}
                                </div>
                                <div class="lname">
                                    {{ form_widget(form.telephone) }}
                                </div>
                            </div>
                            <div class="info-area">
                                <div class="mail-address">
                                    {{ form_widget(form.email) }}
                                </div>
                            </div>
                            <div class="select-service">
                                {{ form_widget(form.metier) }}
                            </div>
                            <div class="message">
                                {{ form_widget(form.message) }}
                            </div>
                            <div class="submit-btn">
                                <button type="submit">Envoyer votre message</button>
                            </div>
                        {{ form_end(form) }}
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- Contact Section End -->

    {% include '_partials/_mignatures/_testimonial.html.twig' %}

    <!-- Counterup Section -->
    <section class="bixol-ct-section pt-100 pb-100">
        <div class="container">
            <div class="row">
                <div class="col-lg-3 col-md-4 col-6">
                    <div class="bixol-counter-column bx-col-line">
                        <div class="bixol-icon-wrapper">
                            <span><i class="flaticon flaticon-garment"></i></span>
                        </div>
                        <div class="bixol-ct-content">
                            <h3><span class="odometer" data-value="2542">000</span><sup>+</sup></h3>
                            <span class="subtitle">Client satisfait</span>
                        </div>
                    </div>
                </div>
                <div class="col-lg-3 col-md-4 col-6">
                    <div class="bixol-counter-column bx-col-line">
                        <div class="bixol-icon-wrapper">
                            <span><i class="flaticon flaticon-meeting"></i></span>
                        </div>
                        <div class="bixol-ct-content">
                            <h3><span class="odometer" data-value="282">000</span><sup>+</sup></h3>
                            <span class="subtitle">Expert de notre équipe</span>
                        </div>
                    </div>
                </div>
                <div class="col-lg-3 col-md-4 col-6">
                    <div class="bixol-counter-column bx-col-line">
                        <div class="bixol-icon-wrapper">
                            <span><i class="flaticon flaticon-screen"></i></span>
                        </div>
                        <div class="bixol-ct-content">
                            <h3><span class="odometer" data-value="285">000</span><sup>+</sup></h3>
                            <span class="subtitle">Clients</span>
                        </div>
                    </div>
                </div>
                <div class="col-lg-3 col-md-4 col-6">
                    <div class="bixol-counter-column">
                        <div class="bixol-icon-wrapper">
                            <span><i class="flaticon flaticon-trophy"></i></span>
                        </div>
                        <div class="bixol-ct-content">
                            <h3><span class="odometer" data-value="27">000</span><sup>+</sup></h3>
                            <span class="subtitle">parutions dans les médias</span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- Counterup End -->
{% endblock %}
