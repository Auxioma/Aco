        <header class="bixol-header bg-sticky-white">
            <div class="bixol-info-bar">
                <div class="container">
                    <div class="row align-items-center">
                        <div class="col-sm-8">
                            <div class="bixol-info-left">
                                <p>
                                    {% trans %}S.A.S ACO{% endtrans %}
                                    <span>
                                        {% trans %} Professionnel dans le domaine du nettoyage industriel{% endtrans %}
                                    </span>
                                </p>
                            </div>
                        </div>
                        <div class="col-sm-4">
                            <div class="bixol-info-social">
                                <a href="#"><i class="fab fa-facebook"></i></a>
                                <a href="#"><i class="fab fa-twitter"></i></a>
                                <a href="#"><i class="fab fa-linkedin"></i></a>
                                <a href="#"><i class="fab fa-instagram"></i></a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="bixol-header-top">
                <div class="container">
                    <div class="bixol-header-top-content">
                        <div class="row">
                            <div class="col-lg-6">
                                <div class="bixol-header-top-left">
                                    <div class="bixol-header-item">
                                        <span class="bixol-icon-wrapper">
                                            <i class="flaticon flaticon-phone"></i>
                                        </span>
                                        <div class="bixol-item-content">
                                            <label>{% trans %}Téléphone:{% endtrans %}</label>
                                            <span>04 57 43 83 23 </span>
                                        </div>
                                        <a href="tel:04 57 43 83 23 "></a>
                                    </div>
                                    <div class="bixol-header-item">
                                        <span class="bixol-icon-wrapper">
                                            <i class="flaticon flaticon-alarm-clock"></i>
                                        </span>
                                        <div class="bixol-item-content">
                                            <label>{% trans %}Lundi au vendredi:{% endtrans %}</label>
                                            <span>{% trans %}8:30 - 16:30{% endtrans %}</span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-6">
                                <div class="bixol-header-top-right">
                                    <div class="bixol-header-item">
                                        <span class="bixol-icon-wrapper">
                                            <i class="flaticon flaticon-mail"></i>
                                        </span>
                                        <div class="bixol-item-content">
                                            <label>contact at acoproprete.com</label>
                                            <span>{% trans %}E-mail{% endtrans %}</span>
                                        </div>
                                        <a href="tel:1234563257"></a>
                                    </div>
                                    <div class="bixol-header-item">
                                        <span class="bixol-icon-wrapper">
                                            <i class="flaticon flaticon-pin"></i>
                                        </span>
                                        <div class="bixol-item-content">
                                            <label>76, Rte Nationale</label>
                                            <span>Lugrin</span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="bixol-navigation">
                <div class="container">
                    <div class="bixol-nav">
                        <a href="{{ path('app_homepage') }}" class="bixol-header-logo">
                            <img src="{{ asset('images/logo.png') }}" alt="">
                        </a>
                        <nav class="desktop-menu">
                            <ul itemscope itemtype="https://schema.org/BreadcrumbList">
                                {% block navigation %}
                                    {% for categorie in categories %}
                                        <li itemprop="itemListElement" itemscope itemtype="https://schema.org/ListItem">
                                            <a  itemprop="item" href="{{ url( categorie.slug ) }}">
                                                <span itemprop="name">
                                                    {{ categorie.name }}
                                                </span>
                                            </a>
                                            <meta itemprop="position" content="{{ loop.index }}" />
                                        </li>
                                    {% endfor %}
                                {% endblock %}   
                            </ul>
                        </nav>
                    </div>
                    <div class="bixol-mobile-header">
                        <div class="row align-items-center">
                            <div class="col-6">
                                <a href="{{ path('app_homepage') }}" class="bixol-logo"><img src="{{ asset('images/logo-mobile.png') }}" alt=""></a>
                            </div>
                            <div class="col-6">
                                <div class="bixol-mobile-hamburger">
                                    <span></span>
                                    <span></span>
                                    <span></span>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </header>